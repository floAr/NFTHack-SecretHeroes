{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "description": "display the minter config",
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object"
        }
      }
    },
    {
      "description": "display the number of packs minted",
      "type": "object",
      "required": [
        "packs_minted"
      ],
      "properties": {
        "packs_minted": {
          "type": "object"
        }
      }
    }
  ]
}
